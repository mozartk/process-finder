  "keywords": [
    "process",
    "pid exists",
    "process exists",
    "running processes"
  ],
  "homepage": "https://github.com/mozartk",
  "license": "MIT",
  "authors": [
    {
      "name": ":mozartk",
      "email": "mozartk30@gmail.com",
      "homepage": "https://github.com/mozartk"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "symfony/process": "~2.3|~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^3.7|~4|~5|~6"
  },
  "autoload": {
    "psr-4": {
      "mozartk\\ProcessFinder\\": "src"
    }
  },
  "autoload-dev": {
